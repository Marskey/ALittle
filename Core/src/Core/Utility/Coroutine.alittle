
namespace ALittle;

public struct Thread { }

public class Coroutine
{
    // 恢复协程运行
    public static Resume(ALittle.Thread thread, ...) : bool, ...
    {
        [Language(Lua)]
        return lua.coroutine.resume(thread, ...);
        
        [Language(JavaScript)]
        return javascript.Coroutine.resume(thread, ...);   
    }
}
